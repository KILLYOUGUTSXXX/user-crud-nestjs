{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@utilities/(.*)$": "<rootDir>/../src/_utilities",
    "^@middlewares$": "<rootDir>/../src/_middlewares"
  }
}
